<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gayat.gnits.club5">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView73"
            android:layout_width="362dp"
            android:layout_height="40dp"

            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:text="TELESTORM"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:textColor="?android:attr/textColorLinkInverse"
            android:textSize="34sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="7dp"

            android:layout_marginTop="14dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="520dp"
            android:layout_height="280dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView73"
            app:srcCompat="@drawable/cyber"
            tools:ignore="ContentDescription"
            tools:layout_editor_absoluteX="-18dp"
            tools:layout_editor_absoluteY="48dp" />

        <TextView
            android:id="@+id/textView74"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageView10"
            android:layout_marginTop="20dp"
            android:text="@string/etm"
            android:textColor="@android:color/black"
            android:textSize="24sp"
            android:textStyle="italic" />
        <TextView
            android:id="@+id/textView75"
            android:layout_width="362dp"
            android:layout_height="40dp"
            android:fontFamily="serif-monospace"

            android:text="EVENTS"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:textColor="@android:color/holo_purple"
            android:textSize="24sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="7dp"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/textView74"
            android:layout_alignLeft="@+id/textView73"
            android:layout_alignStart="@+id/textView73" />

        <TextView
            android:id="@+id/textView76"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:text="@string/etmn"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            android:layout_below="@+id/textView75"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="9dp" />

    </RelativeLayout>

</ScrollView>
