<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gayat.gnits.club1">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView44"
            android:layout_width="362dp"
            android:layout_height="40dp"

            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:text="CYBERSURDS"


            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:textColor="?android:attr/textColorLinkInverse"
            android:textSize="34sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="7dp"
            android:layout_marginTop="9dp"

            android:layout_alignLeft="@+id/textView48"
            android:layout_alignStart="@+id/textView48" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="520dp"
            android:layout_height="280dp"
            app:srcCompat="@drawable/cyber"
            tools:ignore="ContentDescription"
            tools:layout_editor_absoluteX="-18dp"
            tools:layout_editor_absoluteY="48dp"
            android:layout_marginTop="14dp"
            android:layout_below="@+id/textView44"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/textView45"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageView3"
            android:layout_marginTop="20dp"
            android:text="@string/cyber"
            android:textColor="@android:color/black"
            android:textSize="24sp"
            android:textStyle="italic" />
        <TextView
            android:id="@+id/textView46"
            android:layout_width="362dp"
            android:layout_height="40dp"
            android:fontFamily="serif-monospace"

            android:text="EVENTS"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:textColor="@android:color/holo_purple"
            android:textSize="24sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="7dp"
            android:layout_marginTop="99dp"
            android:layout_alignTop="@+id/textView45"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
        <TextView
            android:id="@+id/textView47"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cyber1"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            android:layout_below="@+id/textView46"
            android:layout_alignLeft="@+id/textView46"
            android:layout_alignStart="@+id/textView46" />
        <TextView
            android:id="@+id/textView48"
            android:layout_width="362dp"
            android:layout_height="40dp"

            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:text="CSI"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:textColor="?android:attr/textColorLinkInverse"
            android:textSize="34sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="7dp"
            android:layout_below="@+id/textView47"
            android:layout_centerHorizontal="true" />
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="520dp"
            android:layout_height="280dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"

            app:srcCompat="@drawable/csi"
            android:layout_below="@+id/textView48"
            tools:ignore="ContentDescription"
            tools:layout_editor_absoluteX="-18dp"
            tools:layout_editor_absoluteY="48dp" />
        <TextView
            android:id="@+id/textView49"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/csi"
            android:textColor="@android:color/black"
            android:textSize="24sp"
            android:textStyle="italic"
            android:layout_below="@+id/imageView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
        <TextView
            android:id="@+id/textView50"
            android:layout_width="362dp"
            android:layout_height="40dp"
            android:fontFamily="serif-monospace"

            android:text="EVENTS"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:textColor="@android:color/holo_purple"
            android:textSize="24sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="7dp"

            android:layout_below="@+id/textView49"
            android:layout_alignRight="@+id/textView48"
            android:layout_alignEnd="@+id/textView48"
            android:layout_marginRight="10dp"
            android:layout_marginEnd="10dp" />

        <TextView
            android:id="@+id/textView51"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/csi1"
            android:textColor="@android:color/black"
            android:textSize="20sp"
            android:layout_below="@+id/textView50"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="25dp" />


    </RelativeLayout>

</ScrollView>
